{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "generate-ts": "gql-gen  --config codegen.yml",
    "generate": "npm run generate-ts",
    "generate:win": "npm run generate-ts",
    "clean": "rimraf dist",
    "copy-schemas": "cp -rv ./src/graphql/types ./dist/graphql",
    "prebuild": "npm run clean && npm run generate && npm run tslint",
    "build": "npm run tsc  && npm run copy-schemas",
    "build:win": "npm run tsc:win",
    "start": "npm run build && sls offline start --skipCacheInvalidation",
    "start:win": "npm run build:win && sls offline start --skipCacheInvalidation",
    "dev": "tsc --watch & nodemon dist",
    "test": "jest",
    "tsc": "tsc",
    "tsc:win": "tsc",
    "tslint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "precommit-tslint": "tslint --fix -c tslint.json",
    "precommit-prettier": "prettier --write",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "deploy": "npm run build && sls deploy",
    "dynamodb": "serverless dynamodb start",
    "migrate": "serverless dynamodb migrate",
    "installdb": "sls dynamodb install",
    "package": "yarn build && sls package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run precommit-tslint",
      "npm run precommit-prettier"
    ]
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.0.10",
    "@graphql-tools/merge": "^6.0.10",
    "@graphql-tools/schema": "^6.0.10",
    "@graphql-tools/utils": "^6.0.10",
    "apollo-datasource": "^0.7.1",
    "apollo-datasource-rest": "0.9.2",
    "apollo-server-express": "2.14.4",
    "apollo-server-testing": "^2.15.0",
    "aws-sdk": "^2.697.0",
    "bcryptjs": "^2.4.3",
    "core-js": "3.6.5",
    "cors": "^2.8.5",
    "elasticsearch": "16.7.1",
    "express": "^4.17.1",
    "graphql": "^15.2.0",
    "graphql-tools": "6.0.10",
    "http-aws-es": "^6.0.0",
    "injection-js": "^2.3.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "1.7.8",
    "module-alias": "2.2.2",
    "moment-timezone": "^0.5.31",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "2.5.0",
    "uuid": "8.1.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.3",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-operations": "1.15.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/elasticsearch": "^5.0.36",
    "@types/express": "4.17.1",
    "@types/graphql": "14.5.0",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/uuid": "8.0.0",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "husky": "4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "10.2.10",
    "nock": "^12.0.3",
    "node-fetch": "^2.6.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "serverless-dotenv-plugin": "2.4.2",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "6.4.0",
    "ts-jest": "^26.1.1",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "5.0.0",
    "typescript": "3.9.5"
  }
}
